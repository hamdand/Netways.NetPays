{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQConfigurations": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "QoS": 10,
    "DelayInSeconds": 1
  },
  "RedisFusionConfigurations": {
    "IsEnabled": true,
    "ConnectionString": "localhost:6379",
    "InstanceName": "MyCollection",
    "ExpirationTimeSpanInMinutes": 5,
    "AcceptableLanguages": [
      "en-US",
      "en",
      "ar-JO",
      "ar",
      "fr-FR",
      "es-ES"
    ]
  }
}
